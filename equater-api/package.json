{
  "name": "api",
  "version": "2.0.2",
  "description": "",
  "repository": "https://github.com/RobertMenke/equater-api",
  "author": "",
  "engines": {
    "node": "18.9.1"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rm -rf dist",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=test node --expose-gc ./node_modules/.bin/jest --forceExit --detectOpenHandles --logHeapUsage",
    "test:watch": "NODE_ENV=test jest --forceExit --verbose --detectOpenHandles --watch",
    "test:cov": "NODE_ENV=test jest --forceExit --verbose --detectOpenHandles --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs -d ./src/config/typeorm-migrations-data-source.config.ts"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.16",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/bull": "^0.6.1",
    "@nestjs/common": "^9.1.2",
    "@nestjs/core": "^9.1.2",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.1.2",
    "@nestjs/platform-socket.io": "^9.1.2",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.1.2",
    "@sendgrid/mail": "^7.7.0",
    "@socket.io/redis-adapter": "^7.2.0",
    "aws-sdk": "^2.1224.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "bull": "^4.9.0",
    "cache-manager": "^4.1.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "date-fns": "^2.29.3",
    "dinero.js": "^1.9.1",
    "dotenv": "^16.0.2",
    "dwolla-v2": "^3.4.0",
    "fast-levenshtein": "^3.0.0",
    "firebase-admin": "^11.0.1",
    "fp-ts": "^2.12.3",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "plaid": "^9.7.0",
    "redis": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.7",
    "sharp": "^0.31.0",
    "swagger-ui-express": "^4.5.0",
    "twilio": "^3.82.1",
    "typeorm": "^0.3.10",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@nestjs/cli": "^9.1.3",
    "@nestjs/testing": "^9.1.2",
    "@types/array.prototype.flatmap": "^1.2.2",
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.9",
    "@types/date-fns": "^2.6.0",
    "@types/dinero.js": "^1.9.0",
    "@types/express": "^4.17.14",
    "@types/fast-levenshtein": "^0.0.2",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.23",
    "@types/node-fetch": "^2.6.2",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/serve-static": "^1.15.0",
    "@types/sharp": "^0.31.0",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "jest": "^29.1.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "tslint": "^6.1.3",
    "typeorm-transactional-tests": "^2.0.0",
    "typescript": "^4.8.3"
  },
  "resolutions": {
    "@types/serve-static": "1.13.9"
  },
  "jest": {
    "testTimeout": 30000,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.test.json"
        }
      ]
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "restoreMocks": true,
    "clearMocks": true,
    "resetMocks": true
  }
}
